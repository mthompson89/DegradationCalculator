testFunc <- function(Pinus_resinosaAGS = 0,Acer_SaccharumAGS = 0 ,Betula_alleghaniensisAGS= 0,Betula_PapyriferaAGS=0,
Pinus_resinosa=0,Acer_Saccharum=0,Betula_alleghaniensis=0,Betula_Papyrifera=0,
totBA = 100){
UGS <- c((1-Pinus_resinosaAGS)*(Pinus_resinosa*totBA),
(1-Acer_SaccharumAGS)*(Acer_Saccharum*totBA),
(1-Betula_alleghaniensisAGS)*(Betula_alleghaniensis*totBA),
(1-Betula_PapyriferaAGS)*(Betula_Papyrifera*totBA))
return(UGS)
}
testFunc()
testFunc(Pinus_resinosaAGS = 0.5,Pinus_resinosa = 0.5)
testFunc(Pinus_resinosaAGS = 0,Pinus_resinosa = 0.5)
