year = year,
sumfile = source) %>%
left_join(varsFile, by = "variable") %>%
mutate(town = str_extract(NAME,boundary('word')),
county = str_extract(NAME, "(?<=,\\s)(.*?)(?=,)"),
state = str_extract(NAME, "(?:[^,]+, ){2}(.+)",group = 1)) %>%
pivot_wider(id_cols = GEOID,
names_from = label,
values_from = value) %>%
mutate_if(is.numeric, ~replace(., is.na(.), 0))
}
